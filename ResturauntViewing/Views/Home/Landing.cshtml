@using ResturauntViewing.Models.Models;
@using ResturauntViewing.Models.ViewComponents;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@model List<LeetCodeViewModel>


<div class="text-center">
<h1 id="50" class="display-4">Welcome To Questions Page</h1>
@* <h2>List Questions</h2> *@
     @* https://stackoverflow.com/questions/8507633/mvc-passing-grouped-data-to-view *@
    @foreach (var inType in ViewBag.types){
        <h3>@inType Questions</h3>

        @foreach (LeetCodeViewModel member in @Model.Where(val => val.type == inType))
        {
            <div>
                <button onclick="window.open('@member.descriptionHref','_blank')">@member.Name</button> @* https://stackoverflow.com/questions/5141910/javascript-location-href-to-open-in-new-window-tab *@
            </div>
        }
    }
   
<p>Click i for informational</p>
    <h2>Array Questions</h2>
</div>
